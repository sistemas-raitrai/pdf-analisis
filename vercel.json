{
  "version": 2,
  "builds": [
    {
      "src": "api/analizar.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "estilos.css",
      "use": "@vercel/static"
    },
    {
      "src": "RaiTrai - Logo.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/analizar",
      "dest": "/api/analizar.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}

